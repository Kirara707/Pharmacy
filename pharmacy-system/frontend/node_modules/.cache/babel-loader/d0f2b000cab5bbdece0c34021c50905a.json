             store.dispatch('auth/logout')\r\n              router.push('/login')\r\n            }\r\n            throw new Error(error.response?.data?.message || '获取销售记录失败')\r\n          } finally {\r\n            commit('SET_LOADING', false)\r\n          }\r\n        }\r\n      }\r\n    },\r\n    users: {\r\n      namespaced: true,\r\n      state: {\r\n        list: [],\r\n        loading: false\r\n      },\r\n      mutations: {\r\n        SET_USERS(state, users) {\r\n          state.list = users\r\n        },\r\n        SET_LOADING(state, loading) {\r\n          state.loading = loading\r\n        }\r\n      },\r\n      actions: {\r\n        async fetchUsers({ commit }) {\r\n          commit('SET_LOADING', true)\r\n          try {\r\n            const response = await axios.get('http://localhost:5000/api/users')\r\n            commit('SET_USERS', response.data)\r\n          } catch (error) {\r\n            if (error.response?.status === 401) {\r\n              store.dispatch('auth/logout')\r\n              router.push('/login')\r\n            }\r\n            throw new Error(error.response?.data?.message || '获取用户列表失败')\r\n          } finally {\r\n            commit('SET_LOADING', false)\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n})\r\n\r\nexport default store "],"mappings":";;AAAA,SAASA,WAAW,QAAQ,MAAM;AAClC,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,YAAY;AAEtC,MAAMC,KAAK,GAAGH,WAAW,CAAC;EACxBI,OAAO,EAAE;IACPC,IAAI,EAAE;MACJC,UAAU,EAAE,IAAI;MAChBC,KAAK,EAAE;QACLC,KAAK,EAAEC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,IAAI,IAAI;QAC5CC,IAAI,EAAEC,IAAI,CAACC,KAAK,CAACJ,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC,IAAI;MACpD,CAAC;MACDI,SAAS,EAAE;QACTC,SAASA,CAACR,KAAK,EAAEC,KAAK,EAAE;UACtBD,KAAK,CAACC,KAAK,GAAGA,KAAK;UACnBC,YAAY,CAACO,OAAO,CAAC,OAAO,EAAER,KAAK,CAAC;QACtC,CAAC;QACDS,QAAQA,CAACV,KAAK,EAAEI,IAAI,EAAE;UACpBJ,KAAK,CAACI,IAAI,GAAGA,IAAI;UACjBF,YAAY,CAACO,OAAO,CAAC,MAAM,EAAEJ,IAAI,CAACM,SAAS,CAACP,IAAI,CAAC,CAAC;QACpD,CAAC;QACDQ,UAAUA,CAACZ,KAAK,EAAE;UAChBA,KAAK,CAA